int main()
{
    int i,j,k;
    i = 0;
    while(i<5)
    {
        j = 0;
        while(j<3)
        {
            j++;
            if(j==1)
            {
                break;
            }
        }
        write(j);
        if(i==3)
        {
            break;
        }
        i++;
    }
